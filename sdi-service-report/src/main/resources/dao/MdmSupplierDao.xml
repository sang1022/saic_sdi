<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.saic.ndms.sdi.report.dao.MdmSupplierDao" >
	<update id="mergeSupplierInfo"	parameterType="com.saic.ndms.sdi.report.po.MdmSupplierPO">
		INSERT INTO TM_SUPPLIER  (SUPPLIER_ID, SUPPLIER_DUNS, SUPPLIER_NAME) VALUES (#{supplierId}, #{supplierDuns}, #{supplierName})
	</update>

	<update id="deleteSupplierInfo"	parameterType="java.lang.String">
		DELETE FROM TM_SUPPLIER  WHERE SUPPLIER_DUNS  = #{supplierDuns}
	</update>
	
	<select id="findSupplierCounts"	resultType = "java.lang.Long">
		SELECT COUNT(1) FROM TM_SUPPLIER  
	</select>	
</mapper>